:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5e5a8a102240 .scope module, "test" "test" 2 3;
 .timescale -9 -12;
v0x5e5a8a114c20_0 .var "a", 0 0;
v0x5e5a8a114cf0_0 .var "b", 0 0;
v0x5e5a8a114dc0_0 .net "out", 0 0, L_0x5e5a8a114ec0;  1 drivers
S_0x5e5a8a1023d0 .scope module, "and1" "and_gate" 2 6, 3 1 0, S_0x5e5a8a102240;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "in1";
    .port_info 1 /INPUT 1 "in2";
    .port_info 2 /OUTPUT 1 "out";
L_0x5e5a8a114ec0 .functor AND 1, v0x5e5a8a114c20_0, v0x5e5a8a114cf0_0, C4<1>, C4<1>;
v0x5e5a8a102600_0 .net "in1", 0 0, v0x5e5a8a114c20_0;  1 drivers
v0x5e5a8a114a40_0 .net "in2", 0 0, v0x5e5a8a114cf0_0;  1 drivers
v0x5e5a8a114b00_0 .net "out", 0 0, L_0x5e5a8a114ec0;  alias, 1 drivers
    .scope S_0x5e5a8a102240;
T_0 ;
    %vpi_call 2 9 "$dumpfile", "and_gate_dump.vcd" {0 0 0};
    %vpi_call 2 10 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5e5a8a102240 {0 0 0};
    %vpi_call 2 11 "$monitor", "At time %t: a = %b, b = %b, y = %b", $time, v0x5e5a8a114c20_0, v0x5e5a8a114cf0_0, v0x5e5a8a114dc0_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5e5a8a114c20_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5e5a8a114cf0_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5e5a8a114c20_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5e5a8a114cf0_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5e5a8a114c20_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5e5a8a114cf0_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5e5a8a114c20_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5e5a8a114cf0_0, 0, 1;
    %delay 10000, 0;
    %vpi_call 2 16 "$finish" {0 0 0};
    %end;
    .thread T_0;
